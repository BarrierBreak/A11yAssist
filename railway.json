{
  "services": {
    "a11yassist-backend": {
      "runtime": "python",
      "plan": "starter",
      "build": {
        "commands": [
          "pip install -r requirements.txt"
        ]
      },
      "start": "gunicorn -k gevent --worker-connections 1000 --timeout 0 app:app",
      "environment": {
        "BarrierBreak": "your-random-secret-key-here",
        "RENDER": "true"
      }
    }
  }
}
